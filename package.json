{
  "name": "chatall",
  "version": "1.84.109",
  "description": "Chat with multiple AI bots and discover the best.",
  "author": {
    "name": "Sun Zhigang",
    "email": "sunner@gmail.com",
    "url": "http://sunner.cn"
  },
  "repository": {
    "type": "git",
