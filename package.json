    "@vue/cli-service": "^5.0.8",
    "electron": "^33.4.11",
    "electron-devtools-installer": "^4.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^8.0.3",
    "lint-staged": "^16.0.0",
